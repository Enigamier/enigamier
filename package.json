  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
